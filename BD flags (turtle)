import turtle
bd=turtle.Turtle()

bd.speed(5)
bd.color('green')

bd.penup()
bd.backward(200)
bd.pendown()

bd.begin_fill()
bd.fd(400)
bd.left(90)
bd.fd(240)
bd.left(90)
bd.fd(400)
bd.left(90)
bd.fd(240)
bd.end_fill()

bd.penup()
bd.left(90)
bd.fd(250)
bd.left(90)
bd.fd(120)
bd.pendown()


bd.begin_fill()
bd.color('red')
bd.circle(70)
bd.end_fill()

bd.hideturtle()

turtle.done()
